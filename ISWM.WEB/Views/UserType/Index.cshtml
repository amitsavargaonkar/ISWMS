@model IEnumerable<ISWM.WEB.BusinessServices.userType_master>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.user_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user_type_desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActivie)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.user_type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user_type_desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActivie)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.user_type_id  }) |          
            @Html.ActionLink("Delete", "Delete", new { id = item.user_type_id })
        </td>
    </tr>
}

</table>
